import { TopBar } from "./topbar.60";

Market := Rectangle {
    background: #12171d;
    vertical-stretch: 1;
    horizontal-stretch: 1;
    clip: true;

    callback forum-button-clicked();

    VerticalLayout { 
        padding-left: 20phx;
        padding-right: 20phx;
        TopBar {
            forum-click() => { 
                root.forum-button-clicked()
            }
        }
        Rectangle { height: 15phx; }
        
    }
}